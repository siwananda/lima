{
  "name": "lima",
  "version": "0.0.1-SNAPSHOT",
  "description": "The task management app",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf node_modules doc src/main/webapp/static && npm cache clean",
    "watch": "webpack --watch --progress --profile --colors --display-error-details --display-cached --hot",
    "clean-install": "npm run clean && npm install",
    "clean-start": "npm run clean && npm start",
    "build": "rimraf src/main/webapp/static && webpack --webpack-build --progress --profile --colors --display-error-details --display-cached",
    "server": "webpack-dev-server --inline --progress --colors --display-error-details --display-cached --hot --port=3010",
    "start": "npm run server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/siwananda/lima.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/siwananda/lima/issues"
  },
  "homepage": "https://github.com/siwananda/lima#readme",
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "^0.23.1",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.9.0",
    "node-sass": "^3.4.2",
    "postcss-loader": "^0.8.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "angular": "^1.5.0",
    "angular-animate": "^1.5.0",
    "angular-sanitize": "^1.5.0",
    "angular-ui-router": "^0.2.18",
    "font-awesome": "^4.5.0",
    "jquery": "^2.2.0",
    "lodash": "^3.10.1",
    "nprogress-npm": "^0.1.3",
    "restangular": "^1.5.2"
  }
}
